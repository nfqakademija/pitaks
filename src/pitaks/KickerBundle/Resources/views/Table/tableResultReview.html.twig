   <div class="tableFormat">
    {% if tableStatus  %}
        <div class="row">
            <div class="row ">
                <div class="col-xs-5 tableLeftSide">
                    {% if team1 %}
                        <div class="team-name">{{ team1.name}}</div>
                    {% else %}
                      <br/>
                    {% endif %}
                    <div class="player-card">
                    {% if user11 %}
                        {%  if( user11.image) %}
                            <img class="table-user-photo" src="{{ asset( user11.image.getImageAddress() )  }}">
                        {% else %}
                            <img class="table-user-photo" src="{{ asset('images/anonymous.png') }}">
                        {% endif %}
                        <span class="player-name"> {{ user11.username }}</span>
                    {% else %}
                        <span class="player-name">  anonymous</span>
                    {% endif %}
                    </div>
                </div>
                <div class="col-xs-2">
                </div>
                <div class="col-xs-5">
                    {% if team2 %}
                     <div class="team-name">{{ team2.name}}</div>
                    {% else %}
                        <br/>
                    {% endif %}
                    <div class="player-card">
                    {% if user21 %}
                        {%  if( user21.image) %}
                            <img class="table-user-photo" src="{{ asset( user21.image.getImageAddress() )  }}">
                        {% else %}
                            <img class="table-user-photo" src="{{ asset('images/anonymous.png') }}">
                        {% endif %}
                           <span class="player-name">{{ user21.username }}</span>
                    {% else %}
                            <span class="player-name"> anonymous</span>
                    {% endif %}
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-xs-5">
                </div>
                <div class="col-xs-2 tableCenterSide">
                   <span class="score">{{  game.scoreTeam1}} : {{ game.scoreTeam2}}</span>
                </div>
                <div class="col-xs-5">
                </div>
            </div>
            <div class="row">
                <div class="col-xs-5 tableLeftSide">
                    <div class="player-card">
                {% if user12 %}
                    {%  if( user12.image) %}
                        <img class="table-user-photo" src="{{ asset( user12.image.getImageAddress() )  }}">
                    {% else %}
                        <img class="table-user-photo" src="{{ asset('images/anonymous.png') }}">
                    {% endif %}
                        <span class="player-name">{{ user12.username }}</span>
                    {% else %}
                         <span class="player-name">anonymous</span>
                {% endif %}
                    </div>
                </div>
                <div class="col-xs-2">
                </div>
                <div class="col-xs-5">
                    <div class="player-card">
                    {% if user22 %}
                        {%  if( user22.image) %}
                            <img class="table-user-photo" src="{{ asset( user22.image.getImageAddress() )  }}">
                        {% else %}
                            <img class="table-user-photo" src="{{ asset('images/anonymous.png') }}">
                        {% endif %}
                        <span class="player-name">{{ user22.username }}</span>
                    {% else %}
                        <span class="player-name">anonymous</span>
                    {% endif %}
                    </div>
                </div>
            </div>
        </div>
{% else %}
    <div class="tableCenterSide ">
        <span class="score">FREE</span>
    </div>
{% endif %}
