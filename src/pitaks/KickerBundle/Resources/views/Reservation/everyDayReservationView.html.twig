
    <table class="table" >
        <tr>
            <th>Start</th>
            <th>End</th>
        </tr>
        {% for item in list %}
            <tr{% if item.isFree %} class="bg-success" {% else %} style="background-color: red" {% endif %}>
                <td> {{ item.reservationStart|date("H:i")}} </td>
                <td> {{ item.reservationEnd|date("H:i")}} </td>
            </tr>
        {% endfor %}
    </table>
